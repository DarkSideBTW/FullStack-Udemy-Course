<%- include('partials/header') %>

<h1>Home</h1>

<p><%= startingContent %></p>

<% posts.forEach((post) => { %>
    <h1> <%= post.title.substring(0, 100) %> </h1>
    <p><%= post.content.substring(0, 100) + " ..." %> <a href='/posts/<%= post.title %>'>Read More</a></p>
<% }) %>

<%-include('partials/footer') %>
